            <div class="fb">
            {% if content.comment_script == '' %}
              <div id="fb-root"></div>
              <script>(function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) return;
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=117393808353070";
                fjs.parentNode.insertBefore(js, fjs);
              }(document, 'script', 'facebook-jssdk'));</script>

              {% if content.facebook_url  != '' %}
                <div class="fb-comments" data-href="{{ content.facebook_url }}" data-num-posts="3" data-width="695"></div>

