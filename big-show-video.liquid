                            </h1>
                           {% endif %}
                    </div><!-- Logo End -->

                    <!-- Call Us -->
                    <div class="span6 call">
                        <span>{{ content.phone_number }}</span>
                    </div><!-- Call Us END -->
                </div>
            </div>
            <!-- Head Container END -->
        </header>
        <div class="container margin">



        </div>

        <div class="clear"></div><!-- CLEAR -->

        <!-- Start Header Break Line -->
        <div class="container_12">
            <div class="grid_12">
              <hr>
            </div>
        </div><!-- Header Break Line END -->

        <div class="clear"></div><!-- CLEAR -->

        <!-- Start Teaser -->
        <div class="container_12 ">

            <!-- Start Centered Text -->
            <div class="grid_12">

                <!-- Heading -->
                <h1 class="heading">{{ content.tagline }}</h1>

                {% if content.sub_tagline != '' %}
                <!-- Subheading -->
                <span class="subhead">{{ content.sub_tagline }}</span>
        {% endif %}
            </div><!-- Centered Text END -->

        </div><!-- Teaser END -->

    <div class="clear"></div>

    <!-- Start Container 12 -->
    <div id="main_content" class="container_12">

        <!-- Start Video -->
        <div class="grid_8 featured">

            {{content.video_link_or_embed | fallback: 'http://vimeo.com/51303005' | video: 620, 465}}

        </div><!-- Video END -->

        <!-- Start newsletter Signup -->
        <div class="grid_4">

            <!-- Start Background Image -->
            <div class="signup">

                <!-- Start Newsletter Box -->
                <div class="newsletter">

                    {{ content.description }}
                    <!-- Start Form -->
                    <div class="searchDiv">
                      {% if content.signup_or_click_through == 'Signup' %}
                        {% form %}
                            {% for element in form_elements %}
                                {% assign req_star = '' %}
                                {% if element.required_class == 'required' %}
                                  {% assign req_star = '*' %}
                                {% endif %}
                                {% case element.field_type %}
                                {% when 'email' %}
                                      <input class="inputbg {{element.required_class}}" id="email" name="email" type="email" data-label-text = "{{ element.label }}"  placeholder="{{element.placeholder}} {{req_star}}" />
                                      {% when 'text' %}
                                      <input class="inputbg {{element.required_class}}" id="{{element.name}}" name="{{element.name}}" placeholder="{{element.placeholder}} {{req_star}}" type="text" data-label-text="{{element.label}}" />
                                      {% when 'textarea' %}
                                      <textarea id="{{element.name}}" class="inputbg {{element.required_class}}" name="{{element.name}}" rows=3 placeholder="{{element.placeholder}} {{req_star}}" data-label-text="{{element.lable}}"></textarea>
                                      {% when 'choice' %}
                                      <select id="{{element.name}}" name="{{element.name}}" class="inputbg" placeholder="{{element.placeholder}}" data-label-text="{{element.label}}">
                                        {% for choice in element.choices %}
                                        <option>{{choice}}</option>
                                        {% endfor %}
                                     </select>
                                     {% when 'checkbox' %}
                                     <label>
                                       <input type="checkbox" class="inputbg" id="{{element.name}}" name="{{element.name}}" data-label-text = "{{element.label}}" />
                                       {{element.placeholder}}  {{req_star}}
                                    </label>
                                {% endcase %}
                            {% endfor %}
                            <div class="clear"></div>
                            <input type="submit" class="submit button" id="submit_button" value="{{ content.button_text }}" />
                        {% endform %}

                        <div class="clear"></div>
                        {% response %}
                            <div class="response">
                              {% common_thankyou_share %}
                            </div>
                        {% endresponse %}
                        {% else %}
                          <a href="{{ content.click_through_link }}" class="click-through submit button" id="signup" title="{{ content.button_text }}">{{ content.button_text }}</a>
                        {% endif %}
                        <div class="clear"></div>
                        <div id="kol_fade"></div>
                    </div><!-- Form END -->

                    <div class="clear"></div><!-- CLEAR -->


                </div><!-- Newsletter Box END -->

            </div><!-- Background Image END-->

        </div><!-- Newsletter Signup END -->

        <div class="clear"></div><!-- CLEAR -->

        {% if content.feature_headline_3 != '' %}
          {% assign grid = 'grid_4' %}
