        <header>
            <!-- Start Head Container -->
            <div class="container">
                <div class="row">
                    <!-- Logo -->
                    <div class="span6">
                          {% if content.logo_image != '' %}
                            <h1 class="logo image">
                              <a href="{{ page_url }}" class='ie6fix'>
                                <img src="{{ content.logo_image }}" alt="{{content.title}}" class="logo_img" />
                                    </a>
                                  <span class="slogan image">{{ content.title }}</span>
                                  </h1>
                          {% else %}
                            <h1 class="logo">
